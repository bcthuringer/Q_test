# Example environment variables file
# Copy this file to .env and fill in the values with your own configuration
# DO NOT commit the actual .env file to version control

# AWS Region
REACT_APP_AWS_REGION=us-east-1

# Cognito User Pool (replace with your own values)
REACT_APP_USER_POOL_ID=us-east-1_example
REACT_APP_USER_POOL_CLIENT_ID=example123456789

# API Gateway (replace with your own value)
REACT_APP_API_ENDPOINT=https://example.execute-api.us-east-1.amazonaws.com/prod

# S3 Buckets (replace with your own values)
REACT_APP_WEBSITE_BUCKET=example-website-bucket
REACT_APP_MEDIA_BUCKET=example-media-bucket

# CloudFront (replace with your own value)
REACT_APP_CLOUDFRONT_DOMAIN=example.cloudfront.net
